<h1>Listing musics</h1>

<table>
  <thead>
    <tr>
      <th>Track</th>
      <th>Artist</th>
      <th>Album</th>
      <th>Time</th>
      <th>Musician</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @musics.each do |music| %>
      <tr>
        <td><%= music.track %></td>
        <td><%= music.artist %></td>
        <td><%= music.album %></td>
        <td><%= music.time %></td>
        <td><%= music.musician_id %></td>
        <td><%= link_to 'Show', music %></td>
        <td><%= link_to 'Edit', edit_music_path(music) %></td>
        <td><%= link_to 'Destroy', music, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Music', new_music_path %>
